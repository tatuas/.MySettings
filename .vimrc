syntax on
set tabstop=4
set softtabstop=4
set autoindent
set expandtab
set shiftwidth=4
set number
set smartindent
set nowrap
set tw=0
set title
set showmatch
set wrapscan
set smartcase
set ignorecase

inoremap { {}<LEFT>
inoremap [ []<LEFT>
inoremap ( ()<LEFT>
inoremap " ""<LEFT>
inoremap ' ''<LEFT>
